/* import print base styles | Basisformatierung f√ºr Drucklayout einbinden */
@import url(../core/print_base.css);

/* #col1 - 0
** #col2 - x
** #col3 - 0
*/
@media print
{
  #col1 {display:none}
  #col3 {display:none}

  #col2, #col2_content { float:none; width: 100%; margin: 0; padding: 0; border: 0}
}